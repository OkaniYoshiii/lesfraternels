{
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Lib\\": "lib/"
        },
        "files": [
            "config/config.php",
            "config/routes/routes.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpbench/phpbench": "^1.4"
    }
}
